{% extends "base.html.twig" %}

{% block body %}
    <div>
        <h1>Register!</h1>

        {{ form_start(form) }}
        {{ form_row(form.username) }}
        {{ form_row(form.plainPassword.first) }}
        {{ form_row(form.plainPassword.second) }}
        <button type="submit" formnovalidate>Register</button>
        {{ form_end(form) }}
    </div>
{% endblock %}